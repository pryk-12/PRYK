  <data name="customSqlQuery1.Sql" xml:space="preserve">
    <value>SELECT dbo.MOVIMIENTOS.ID_MOVIMIENTO, 
       dbo.MOVIMIENTOS.ID_MOTIVO, 
	   dbo.MOTIVOS_MOVIMIENTOS.DESCRIPCION AS MOTIVO, 
	   dbo.MOTIVOS_MOVIMIENTOS.TIPO_MOVIMIENTO, 
       dbo.MOVIMIENTOS.ID_ARTICULO, 
	   dbo.ARTICULOS.REFERENCIA, 
	   dbo.ARTICULOS.DESCRIPCION AS ARTICULO, 
	   dbo.MOVIMIENTOS.CANTIDAD, 
	   dbo.MOVIMIENTOS.OBSERVACION, 
	   CASE dbo.MOVIMIENTOS.ESTADO
	   WHEN 'V' THEN 'Vigente'
	   WHEN 'A' THEN 'Anulado'
	   END ESTADO,
       dbo.MOVIMIENTOS.CREADO_POR, 
	   dbo.MOVIMIENTOS.FECHA_CREADO, 
	   dbo.MOVIMIENTOS.ANULADO_POR, 
	   dbo.MOVIMIENTOS.FECHA_ANULADO, 
	   dbo.MOVIMIENTOS.NOTA_ANULACION,
	   (SELECT SUM(A.CANTIDAD) FROM  dbo.MOVIMIENTOS A WHERE  A.ID_MOTIVO=dbo.MOTIVOS_MOVIMIENTOS.ID_MOTIVO  AND A.ESTADO='V' AND  dbo.MOTIVOS_MOVIMIENTOS.TIPO_MOVIMIENTO='Entrada') CANT_ENTRADA,
	   (SELECT SUM(A.CANTIDAD) FROM  dbo.MOVIMIENTOS A WHERE  A.ID_MOTIVO=dbo.MOTIVOS_MOVIMIENTOS.ID_MOTIVO AND A.ESTADO='V' AND dbo.MOTIVOS_MOVIMIENTOS.TIPO_MOVIMIENTO='Salida') CANT_SALIDA
FROM   dbo.MOVIMIENTOS 
       INNER JOIN dbo.MOTIVOS_MOVIMIENTOS ON dbo.MOVIMIENTOS.ID_MOTIVO = dbo.MOTIVOS_MOVIMIENTOS.ID_MOTIVO 
	   INNER JOIN dbo.ARTICULOS ON dbo.MOVIMIENTOS.ID_ARTICULO = dbo.ARTICULOS.ID_ARTICULO
	   WHERE dbo.MOVIMIENTOS.ESTADO='V'</value>
  </data>
  <data name="customSqlQuery2.Sql" xml:space="preserve">
    <value>SELECT        dbo.EMPRESA.ID_EMPRESA, dbo.EMPRESA.NOMBRE, dbo.EMPRESA.DIRECCION, dbo.EMPRESA.ID_TIPO_IDENTIFICACION, dbo.TIPOS_IDENTIFICACIONES.DESCRIPCION, 
                         dbo.EMPRESA.NUMERO_IDENTIFICACION, dbo.EMPRESA.TELEFONO, dbo.EMPRESA.CELULAR, dbo.EMPRESA.FAX, dbo.EMPRESA.EMAIL, dbo.EMPRESA.PAGINA_WEB, dbo.EMPRESA.ESLOGAN, 
                         dbo.EMPRESA.IMPRESORA, dbo.EMPRESA.LOGO
FROM            dbo.EMPRESA INNER JOIN
                         dbo.TIPOS_IDENTIFICACIONES ON dbo.EMPRESA.ID_TIPO_IDENTIFICACION = dbo.TIPOS_IDENTIFICACIONES.ID_TIPO_IDENTIFICACION</value>
  </data>
